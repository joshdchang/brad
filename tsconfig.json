{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": ".",
    "lib": [
      "ES2021",
      "DOM"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/keystatic-config": [
        "keystatic.config.ts"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": [
    "src",
    "*.ts"
  ],
  "exclude": [
    "node_modules",
    "**/node_modules/*",
    ".vscode",
    "dist"
  ]
}